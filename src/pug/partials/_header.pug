	// Bootstrap
	link(href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet" onerror="loadBootstrap()" id="loadBootstrap")
	script.
		function loadBootstrap() {
			$( "#loadBootstrap" ).append('<link href="#{URLbase}css/plugins/bootstrap.min.css" rel="stylesheet">');
		}


	// Plugins CSS
	link(href=URLbase+"css/plugins/easy-autocomplete.min.css" rel="stylesheet")
