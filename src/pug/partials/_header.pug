	link(rel="apple-touch-icon" sizes="152x152" href=URLbase+"images/favicons/apple-icon-152x152.png")
	link(rel="apple-touch-icon" sizes="180x180" href=URLbase+"images/favicons/apple-icon-180x180.png")
	link(rel="icon" type="image/png" sizes="192x192" href=URLbase+"images/favicons/android-icon-192x192.png")
	link(rel="icon" type="image/png" sizes="32x32" href=URLbase+"images/favicons/favicon-32x32.png")
	link(rel="icon" type="image/png" sizes="96x96" href=URLbase+"images/favicons/favicon-96x96.png")
